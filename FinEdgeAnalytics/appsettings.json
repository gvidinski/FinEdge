{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		},
		"Console": {
			"IncludeScopes": false,
			"FormatterName": "simple",
			"FormatterOptions": {
				"TimestampFormat": "[yyyy-MM-dd HH:mm:ss.fff] ",
				"IncludeTimestamp": true
			}
		}
	},
	"AllowedHosts": "*",
	"ConnectionStrings": {
		"DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=FinEdge;Trusted_Connection=True;MultipleActiveResultSets=true"

	},
	"CsvExtractor": {
		"CsvFilePath": "Data/test.csv",
		"CsvBatchSize": 6,
		"CsvSeparator": ","
	},
	"CsvTransformer": {
		"DescriptionLength": 255
	},
	"ApiExtractor": {
		"ApiUrl": "https://localhost:8000/api/transactions"
	}
}
